{
    "name": "iclubs",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "ts-node-dev --transpile-only --ignore-watch [node_modules,__tests__] src/server.ts",
        "test": "jest --detectOpenHandles",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "start": "ts-node src/index.ts"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.9.0",
        "@types/supertest": "^2.0.11",
        "@types/typescript": "^2.0.0",
        "@types/uuid": "^8.3.1",
        "jest": "^27.1.1",
        "supertest": "^6.1.6",
        "ts-jest": "^27.0.5",
        "ts-node": "3.3.0",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.11.0",
        "typescript": "3.3.3333"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "mysql": "^2.14.1",
        "reflect-metadata": "^0.1.13",
        "sqlite3": "^5.0.2",
        "typeorm": "^0.2.37",
        "uuid": "^8.3.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "globals": {
            "ts-jest": {
                "tsconfig": "tsconfig.json"
            }
        },
        "testMatch": [
            "**/__tests__/*.+(ts|tsx|js)"
        ]
    }
}
