{
    "name": "iclubs",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "ts-node-dev --transpile-only --ignore-watch [node_modules,__tests__] src/server.ts",
        "test": "jest"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.7.11",
        "@types/supertest": "^2.0.11",
        "@types/typescript": "^2.0.0",
        "@types/uuid": "^8.3.1",
        "jest": "^27.1.1",
        "jest-extended": "^0.11.5",
        "supertest": "^6.1.6",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.4.2"
    },
    "dependencies": {
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "ts-jest": "^27.0.5",
        "uuid": "^8.3.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "globals": {
            "ts-jest": {
                "tsconfig": "tsconfig.json"
            }
        },
        "testMatch": [
            "**/__tests__/*.+(ts|tsx|js)"
        ],
        "setupFilesAfterEnv": ["jest-extended"]
    }
}